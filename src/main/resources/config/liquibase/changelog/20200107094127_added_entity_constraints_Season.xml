<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Season.
    -->
    <changeSet id="20200107094127-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="team_id_id"
                                 baseTableName="season"
                                 constraintName="fk_season_team_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="season_id"
                                 baseTableName="season_players"
                                 constraintName="fk_season_players_season_id"
                                 referencedColumnNames="id"
                                 referencedTableName="season"/>

        <addForeignKeyConstraint baseColumnNames="players_id"
                                 baseTableName="season_players"
                                 constraintName="fk_season_players_players_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>
        
    </changeSet>
</databaseChangeLog>
